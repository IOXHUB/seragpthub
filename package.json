{
  "name": "librechat",
  "version": "0.7.0",
  "description": "An Open Source ChatGPT Alternative",
  "type": "module",
  "main": "api/server/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:api": "npm run --workspace=api dev",
    "dev:client": "npm run --workspace=client dev",
    "start": "npm run --workspace=api start",
    "start:client": "npm run --workspace=client start",
    "build": "npm run build:client && npm run build:api",
    "build:api": "npm run --workspace=api build",
    "build:client": "npm run --workspace=client build",
    "build:local": "npm run build:client && npm run build:api",
    "frontend": "npm ci && npm run build:client",
    "backend": "npm run --workspace=api start",
    "deploy": "npm run build && npm run start",
    "setup": "node scripts/setup.js",
    "test": "npm run --workspace=api test",
    "test:client": "npm run --workspace=client test",
    "test:api": "npm run --workspace=api test",
    "test:e2e": "npm run --workspace=e2e test",
    "lint": "npm run --workspace=api lint && npm run --workspace=client lint",
    "lint:fix": "npm run --workspace=api lint:fix && npm run --workspace=client lint:fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "docker:build": "docker build -t librechat .",
    "docker:run": "docker run -p 3080:3080 --env-file .env librechat",
    "postinstall": "npm run setup"
  },
  "workspaces": [
    "api",
    "client",
    "packages/*"
  ],
  "keywords": [
    "chatgpt",
    "openai",
    "ai",
    "chat",
    "gpt",
    "react",
    "node",
    "express",
    "mongodb",
    "docker"
  ],
  "author": "Danny Avila",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danny-avila/LibreChat.git"
  },
  "bugs": {
    "url": "https://github.com/danny-avila/LibreChat/issues"
  },
  "homepage": "https://librechat.ai",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}